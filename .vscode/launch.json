    "version": "0.2.0",
    "configurations": [
        {
            "type": "chrome",
            "request": "launch",
            "name": "Launch localhost",
            "url": "http://localhost:8091/",
            "webRoot": "${workspaceFolder}/src",
            "runtimeArgs": [
                "--new-window", //Open in new window
            ],
            "sourceMaps": true,
            "userDataDir": true,
            "runtimeExecutable": "C:/Users/hspcadmin/AppData/Local/Google/Chrome/Application/chrome.exe"
        }
    ]
}